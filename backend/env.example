# 环境变量配置示例

# 阿里云DashScope API密钥
AI_DASHSCOPE_API_KEY=your-dashscope-api-key-here

# 数据库配置（生产环境）
DB_HOST=localhost
DB_PORT=3306
DB_NAME=math_agent
DB_USERNAME=root
DB_PASSWORD=your-password

# 应用配置
APP_PORT=8080
APP_ENV=development

# 日志配置
LOG_LEVEL=INFO
LOG_FILE_PATH=/var/log/math-agent/

# 文件存储配置
FILE_STORAGE_PATH=/var/lib/math-agent/files/
MAX_FILE_SIZE=100MB
